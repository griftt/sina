<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC

        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.entity.Friend" table="t_friend">

       <id name="id" type="integer" column="id">
            <generator class="identity"/>
       </id>
       <property name="userId" type="integer" column="userId" />
       <property name="friendId" type="integer" column="friendId"/>
       <property name="name" type="string" column="name"/>
        <property name="focus" type="integer" column="focus"/>
         <property name="beFocus" type="integer" column="beFocus"/>
    </class>

</hibernate-mapping>